{
  "version": 2,
  "name": "name-of-your-vercel-app",
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/script.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "files": [
    "index.html"
  ]
}
